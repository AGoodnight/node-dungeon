{
    "compilerOptions": {
        "rootDir": ".",
        "target": "es2016",
        "module": "commonjs",
        "resolveJsonModule": true,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUncheckedIndexedAccess": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules"
    ]
}